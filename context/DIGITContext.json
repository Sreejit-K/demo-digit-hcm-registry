{
  "@context": {
    "@version": 1.1,
    "@protected": true,
    "id": "@id",
    "type": "@type",
    "schema": "https://schema.org/",
    "pledge": "https://raw.githubusercontent.com/Sunbird-RC/demo-donor-registry/main/contexts/vc-context.json",
    "emergency": "https://raw.githubusercontent.com/Sunbird-RC/demo-donor-registry/main/contexts/vc-context.json",
    "ProofOfOrganPledge": {
      "@id": "schema:ProofOfOrganPledge",
      "@context": {
        "id": "@id",
        "@version": 1.1,
        "@protected": true,
        "nonTransferable": "pledge:nonTransferable"
      }
    },
    "Person": {
      "@id": "schema:Person",
      "@context": {
        "@version": 1.1,
        "@protected": true,
        "nottoId": "schema:Text",
        "name": "schema:name",
        "fatherName": "schema:name",
        "dob": "schema:birthDate",
        "gender": "schema:gender",
        "nationality": "schema:nationality",
        "bloodGroup": "schema:Text",
        "address": {
          "@id": "schema:PostalAddress",
          "@context": {
            "@version": 1.1,
            "@protected": true,
            "addressLine1": "schema:streetAddress",
            "addressLine2": "schema:Text",
            "district": "schema:Text",
            "state": "schema:Text",
            "country": "schema:addressCountry",
            "pincode": "schema:postalCode"
          }
        }
      }
    },
    "Emergency": {
      "@id": "pledge:EmergencyDetails",
      "@context": {
        "name": "schema:Text",
        "mobileNumber": "schema:Text",
        "relation": "schema:Text",
        "otherRelation": "schema:Text"
      }
    },
    "OrganPledge": {
      "@id": "pledge:OrganPledge",
      "@context": {
        "organs": "schema:Text",
        "tissues": "schema:Text",
        "additionalOrgans": "schema:Text"
      }
    },
    "eKYC": {
      "@id": "pledge:eKYC",
      "@context": {
        "@version": 1.1,
        "@protected": true,
        "refId": "schema:id",
        "verifier": "schema:id",
        "evidenceDocument": "schema:Text",
        "subjectPresence": "schema:Text"

      }
    }
  }
}